{
    "database": {
        "type": "postgres",
        "host": "mnote-db",
        "port": 5432,
        "user": "mnote",
        "password": "mnote_pass",
        "dbname": "mnote",
        "sslmode": "disable"
    },
    "jwt_secret": "dev-secret-change-me",
    "port": 8080,
    "jwt_ttl_hours": 72,
    "version_max_keep": 10,
    "max_upload_size": 20971520,
    "log_config": {
        "level": "debug",
        "console": true
    },
    "file_store": {
        "type": "local",
        "data": {
            "dir": "/data/uploads"
        }
    },
    "ai_provider": [
        {
            "name": "gemini",
            "type": "gemini",
            "data": {
                "api_key": "YOUR_GEMINI_API_KEY"
            }
        },
        {
            "name": "openrouter",
            "type": "openrouter",
            "data": {
                "api_key": "YOUR_OPENROUTER_API_KEY",
                "http_referer": "https://github.com/xxxsen/mnote",
                "x_title": "mnote"
            }
        },
        {
            "name": "openai",
            "type": "openai",
            "data": {
                "api_key": "YOUR_OPENAI_LIKE_API_KEY",
                "base_url": "https://api.openai.com/v1"
            }
        }
    ],
    "ai": {
        "provider": "gemini",
        "model": "gemini-1.5-flash",
        "timeout": 30,
        "max_input_chars": 32000,
        "polish": [
            {
                "provider": "openrouter",
                "model": "anthropic/claude-3.5-sonnet"
            }
        ],
        "generate": [
            {
                "model": "gemini-1.5-pro"
            }
        ],
        "tagging": [],
        "summary": [],
        "embed": [
            {
                "provider": "gemini",
                "model": "text-embedding-004"
            }
        ]
    },
    "ai_job": {
        "summary_delay_seconds": 300,
        "embedding_delay_seconds": 300
    },
    "oauth": {
        "github": {
            "client_id": "",
            "client_secret": "",
            "redirect_url": "http://localhost/api/v1/auth/oauth/github/callback",
            "scopes": [
                "user:email"
            ]
        },
        "google": {
            "client_id": "",
            "client_secret": "",
            "redirect_url": "http://localhost/api/v1/auth/oauth/google/callback",
            "scopes": [
                "openid",
                "email",
                "profile"
            ]
        }
    },
    "mail": {
        "host": "smtp.example.com",
        "port": 587,
        "username": "",
        "password": "",
        "from": "no-reply@example.com"
    },
    "banner": {
        "enable": false,
        "title": "",
        "wording": "",
        "redirect": ""
    },
    "properties": {
        "enable_github_oauth": false,
        "enable_google_oauth": false,
        "enable_user_register": true,
        "enable_email_register": true,
        "enable_test_mode": true
    }
}
