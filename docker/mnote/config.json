{
    "db_path": "/data/db/data.db",
    "jwt_secret": "dev-secret",
    "port": 8080,
    "jwt_ttl_hours": 72,
    "version_max_keep": 10,
    "log_config": {
        "level": "debug",
        "console": true
    },
    "file_store": {
        "type": "local",
        "data": {
            "dir": "/data/uploads"
        }
    },
    "ai": {
        "provider": "gemini",
        "api_key": "abc",
        "model": "flash",
        "timeout": 10,
        "max_input_chars": 0
    },
    "oauth": {
        "github": {
            "enabled": false,
            "client_id": "",
            "client_secret": "",
            "redirect_url": "http://localhost/api/v1/auth/oauth/github/callback",
            "scopes": ["user:email"]
        },
        "google": {
            "enabled": false,
            "client_id": "",
            "client_secret": "",
            "redirect_url": "http://localhost/api/v1/auth/oauth/google/callback",
            "scopes": ["openid", "email", "profile"]
        }
    }
}
